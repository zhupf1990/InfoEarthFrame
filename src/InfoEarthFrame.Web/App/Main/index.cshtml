@using System.Threading
@using System.Web.Optimization
@using Abp.Timing
<!DOCTYPE html>
<html lang="en" data-ng-app="InfoearthApp">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="{{app.description}}">
    <meta name="keywords" content="app, responsive, angular, bootstrap, dashboard, admin">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta name="HandheldFriendly" content="true" />
    <meta name="apple-touch-fullscreen" content="yes" />
    <title data-ng-bind="pageTitle()">地质灾害防治信息系统</title>
    <!-- Bootstrap -->
    <link href="~/App/bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/App/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/App/bower_components/font-awesome/css/font-awesome.min.css">
    <!-- Themify Icons -->
    <link rel="stylesheet" href="~/App/bower_components/themify-icons/themify-icons.css">
    <!-- Tmaterialdesign Icons -->
    <link rel="stylesheet" href="~/App/bower_components/themify-icons/materialdesignicons.css">
    <!-- Loading Bar -->
    <link rel="stylesheet" href="~/App/bower_components/angular-loading-bar/build/loading-bar.min.css">
    <!-- Animate Css -->
    <link rel="stylesheet" href="~/App/bower_components/animate.css/animate.min.css">
    <!-- 控件样式 -->
    @*<link rel="stylesheet" href="http://192.168.100.69:8081/tmp/v1.0/control.lib.min.css">*@
    <link href="~/App/Main/assets/control.lib.min/control.lib.min.css" rel="stylesheet" />
    <!--<link rel="stylesheet" href="~/cdn/v1.0/control.lib.min.css">-->
    <link rel="stylesheet" data-ng-href="../App/Main/assets/css/themes/{{ app.layout.theme }}.css" />

    <link rel="infoearth icon" href="~/favicon.ico">

    <script type="text/javascript">
        var module, AccessTokenValues = '@ViewBag.token';
        var userinfo = { 'given_name': '@ViewBag.given_name', 'family_name': '@ViewBag.family_name', 'gender': '@ViewBag.gender', 'id': '@ViewBag.id', 'middle_name': '@ViewBag.middle_name' };
        var abp = abp || {}; abp.appPath = '@ApplicationPath';
        </script>
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
            <script src="~/Scripts/others/html5shiv.js"></script>
            <script src="~/Scripts/others/respond.min.js"></script>
        <![endif]-->

</head>
<body ng-controller="AppCtrl">
    <div ui-view id="app" ng-class="{'app-mobile' : app.isMobile, 'app-navbar-fixed' : app.layout.isNavbarFixed, 'app-sidebar-fixed' : app.layout.isSidebarFixed, 'app-sidebar-closed':app.layout.isSidebarClosed, 'app-footer-fixed':app.layout.isFooterFixed}"></div>

    <!-- jQuery -->
    <script src="~/App/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.11.4.js"></script>

    <!-- Fastclick -->
    <script src="~/App/bower_components/fastclick/lib/fastclick.js"></script>
    <!-- Angular -->
    <script src="~/App/bower_components/angular/angular.min.js"></script>
    <script src="~/App/bower_components/angular-cookies/angular-cookies.min.js"></script>
    <script src="~/App/bower_components/angular-animate/angular-animate.min.js"></script>
    <script src="~/App/bower_components/angular-touch/angular-touch.min.js"></script>
    <script src="~/App/bower_components/angular-sanitize/angular-sanitize.min.js"></script>
    <script src="~/App/bower_components/angular-ui-router/release/angular-ui-router.min.js"></script>
    <!-- Angular storage -->
    <script src="~/App/bower_components/ngstorage/ngStorage.min.js"></script>
    <!-- Angular Translate -->
    <script src="~/App/bower_components/angular-translate/angular-translate.min.js"></script>
    <script src="~/App/bower_components/angular-translate-loader-url/angular-translate-loader-url.min.js"></script>
    <script src="~/App/bower_components/angular-translate-loader-static-files/angular-translate-loader-static-files.min.js"></script>
    <script src="~/App/bower_components/angular-translate-storage-local/angular-translate-storage-local.min.js"></script>
    <script src="~/App/bower_components/angular-translate-storage-cookie/angular-translate-storage-cookie.min.js"></script>
    <!-- oclazyload -->
    <script src="~/App/bower_components/oclazyload/dist/ocLazyLoad.min.js"></script>
    <!-- breadcrumb -->
    <script src="~/App/bower_components/angular-breadcrumb/dist/angular-breadcrumb.min.js"></script>
    <!-- UI Bootstrap -->
    <script src="~/App/bower_components/angular-bootstrap/ui-bootstrap-tpls.js"></script>
    <!-- Loading Bar -->
    <script src="~/App/bower_components/angular-loading-bar/build/loading-bar.min.js"></script>
    <!-- Angular Scroll -->
    <script src="~/App/bower_components/angular-scroll/angular-scroll.min.js"></script>
    <!-- raphael-->
    <script src="~/App/bower_components/RaphaelJs/raphael.min.js"></script>
    @Scripts.Render("~/Bundles/App/vendor/js")

       

    <script src="~/api/AbpServiceProxies/GetAll?v=@(Clock.Now.Ticks)"></script>
    <script src="~/api/AbpServiceProxies/GetAll?type=angular&v=@(Clock.Now.Ticks)"></script>
    <script src="~/AbpScripts/GetScripts?v=@(Clock.Now.Ticks)" type="text/javascript"></script>
    <!-- Clip-Two Scripts -->
    <script src="~/App/Main/assets/js/app.js"></script>
    <script src="~/App/Main/assets/js/main.js"></script>
    <script src="~/App/Main/assets/js/config.constant.js"></script>
    @* 导航 *@
       
    <script src="~/App/Main/assets/js/directives/nav/navDirective.js"></script>
    <script src="~/App/Main/assets/js/config.router.js"></script>
    <!-- Clip-Two Directives -->

    @*<script src="http://192.168.100.69:8081/tmp/v1.0/control.lib.min.js"></script>*@
    <script src="~/App/Main/assets/control.lib.min/control.lib.min.js"></script>
    <script src="~/App/Main/assets/js/controllers/mainCtrl.js"></script>

</body>
</html>
